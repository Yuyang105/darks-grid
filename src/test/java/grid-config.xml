<grid>

	<!-- <register name="asd" class="asd"/> -->

	<cluster cluster_name="TEST"/>

	<network 
		bind_host=""
		bind_port="7800"
		bind_port_range="10"
		tcp_nodelay="true"
		keep_alive="true"
