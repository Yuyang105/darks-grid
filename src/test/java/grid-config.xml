<?xml version="1.0" encoding="UTF-8"?>
<grid>

	<storage root_dir="/home/admin/aps-ai/target/grid"/> 
	<!-- 
	<component name="testcom" class="darks.grid.test.TestComponent"/> 
 	-->
	<cluster cluster_name="KELUDEAPS"/>
	 
	<master auto_master="true" min_slave_num="2">
		<task name="demo" class="darks.grid.test.DemoMasterTask" interval="300000" />
	</master>
	
	<task jobs_exec_count="30"/>

	<events default_event_consumer_number="8" system_event_consumer_number="3"/>
	
	<log interval="60000" 
		nodes_info="true" 
	    task_info="true" 
	    exec_job_info="true" 
	    jvm_info="true"
	    stat_info="true"/>

	<network 
		bind_host=""
		bind_port="7800"
		bind_port_range="10"
		tcp_nodelay="true"
		tcp_keep_alive="true"
		tcp_reuse_addr="false"
		recv_timeout="50000"
		connect_timeout="2000"
		server_worker_thread_number="4"
		server_boss_thread_delta="2"
		client_worker_thread_number="4"
		nodes_expire_time="600000"
		cache_history_nodes="false"
		/>

	<alive interval="60000"/>

	<MERGE interval="300000"/>
	
	<TCPPING hosts="localhost:[7800-7805]" interval="300000"/>
	
	<!-- <testcom interval="60000"/> -->
</grid>